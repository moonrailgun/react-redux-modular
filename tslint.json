{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "interface-name": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "semicolon": false,
        "whitespace": false,
        "no-console": [true, "log"]
    },
    "rulesDirectory": []
}
